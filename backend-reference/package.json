{
  "name": "ole-integration-backend",
  "version": "1.0.0",
  "description": "Backend Node.js + Prisma para integração com API Olé TV",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
